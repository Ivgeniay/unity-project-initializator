{
    "tasks": [
        {
            "label": "BuildCli",
            "type": "shell",
            "command": "dotnet build",
            "options": {
                "cwd": "${workspaceFolder}/cll/UnityInitializer.Cli"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "BuildCli Release",
            "type": "shell",
            "command": "dotnet",
            "args": ["build", "--configuration", "Release"],
            "options": {
                "cwd": "${workspaceFolder}/cll/UnityInitializer.Cli"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": "$msCompile"
        }
    ]
}